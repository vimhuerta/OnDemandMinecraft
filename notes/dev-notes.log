Resources:
  MinecraftServerSecurity:
    Type: 'AWS::EC2::SecurityGroup'
    Properties:
      GroupDescription: Allow inbound traffic for Minecraft Server
      GroupName: Minecraft-Server-SG
      SecurityGroupIngress:
      - IpProtocol: tcp
        FromPort: 22
        ToPort: 22
        CidrIp: 0.0.0.0/0
      SecurityGroupIngress:
      - IpProtocol: tcp
        FromPort: 25565
        ToPort: 25565
        CidrIp: 0.0.0.0/0
      SecurityGroupIngress:
      - IpProtocol: udp
        FromPort: 25565
        ToPort: 25565
        CidrIp: 0.0.0.0/0
      SecurityGroupEgress:
      - IpProtocol: -1
        FromPort: 0
        ToPort: 0
        CidrIp: 0.0.0.0/0


Resources:
  MinecraftServerInstance:
    Type: 'AWS::EC2::Instance'
    Properties: {
        IamInstanceProfile: ''
        ImageId: ami-0a40aea49c501581d
        InstanceType: t2.micro
        KeyName: minecraft_key
        SecurityGroups:
            - ''
        }
